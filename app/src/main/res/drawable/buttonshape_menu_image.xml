<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"  >


    <item>
        <shape android:shape="rectangle">


            <corners
                android:radius="100dp"
                />

            <padding
                android:left="0dp"
                android:top="0dp"
                android:right="0dp"
                android:bottom="0dp"
                />
            <size
                android:width="100dp"
                android:height="100dp"
                />
            <stroke
                android:width="5dp"
                android:color="#000000"
                />
        </shape>
    </item>
    <item android:drawable="@drawable/instruction_gameplay_rounded"
        />

</layer-list>